{
  "extends": [
    "config:base",
    "helpers:pinGitHubActionDigests",
    "github>aquaproj/aqua-renovate-config#1.3.0"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +['\"]?version['\"]? *: +['\"]?atlascli/(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=mongodb/mongodb-atlas-cli/atlascli[ \\n]",
        " +['\"]?name['\"]? *: +['\"]?mongodb/mongodb-atlas-cli/atlascli@atlascli/(?<currentValue>[^'\" \\n]+)['\"]?"
      ],
      "extractVersionTemplate": "^atlascli/(?<version>.*)$",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "mongodb/mongodb-atlas-cli/atlascli",
      "packageNameTemplate": "mongodb/mongodb-atlas-cli"
    }
  ]
}
