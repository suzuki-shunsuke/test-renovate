{
  "extends": [
    "config:base",
    "helpers:pinGitHubActionDigests",
    "github>aquaproj/aqua-renovate-config#1.3.0"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +['\"]?version['\"]? *: +['\"]?ipinfo-(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=ipinfo/cli[ \\n]",
        " +['\"]?name['\"]? *: +['\"]?ipinfo/cli@ipinfo-(?<currentValue>[^'\" \\n]+)['\"]?"
      ],
      "extractVersionTemplate": "^ipinfo-(?<version>.*)$",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "ipinfo/cli"
    },
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +['\"]?version['\"]? *: +['\"]?cidr2ip-(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=ipinfo/cli/cidr2ip[ \\n]",
        " +['\"]?name['\"]? *: +['\"]?ipinfo/cli/cidr2ip@cidr2ip-(?<currentValue>[^'\" \\n]+)['\"]?"
      ],
      "extractVersionTemplate": "^cidr2ip-(?<version>.*)$",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "ipinfo/cli/cidr2ip",
      "packageNameTemplate": "ipinfo/cli"
    }
  ]
}
