{
   "extends": [
      "config:recommended"
   ],
   "customManagers": [
      {
         "customType": "regex",
         "datasourceTemplate": "github-tags",
         "depNameTemplate": "suzuki-shunsuke/lintnet",
         "fileMatch": [
            "hello\\.txt"
         ],
         "matchStrings": [
            "github-tags>suzuki-shunsuke/lintnet#(?<currentValue>[^'\" \\n]+)"
         ]
      }
   ]
}
