{
  "extends": [
    "config:base",
    "helpers:pinGitHubActionDigests"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
       "datasourceTemplate": "github-releases",
       "fileMatch": [
          "\\.?aqua\\.ya?ml"
       ],
       "matchStrings": [
          " +(?:version|'version'|\"version\") *: +['\"]?(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=(?<depName>[^\\n]+)",
          " +(?:name|'name'|\"name\") *: +['\"]?(?<depName>(?<packageName>[^'\" @/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" \\n]+)['\"]?"
       ]
    }
  ]
}
