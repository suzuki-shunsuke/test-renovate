{
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
       "datasourceTemplate": "github-tags",
       "fileMatch": [
          "\\.?aqua\\.ya?ml"
       ],
       "matchStrings": [
          " +(?:ref|'ref'|\"ref\") *: +(?<currentDigest>[^'\" \\n]+) +# renovate:( +tag=(?<currentValue>[^'\" \\n]+))? +depName=(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)",
          " +(?:ref|'ref'|\"ref\") *: +'(?<currentDigest>[^'\" \\n]+)' +# renovate:( +tag=(?<currentValue>[^'\" \\n]+))? +depName=(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)",
          " +(?:ref|'ref'|\"ref\") *: +\"(?<currentDigest>[^'\" \\n]+)\" +# renovate:( +tag=(?<currentValue>[^'\" \\n]+))? +depName=(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)"
       ]
    }
  ]
}
