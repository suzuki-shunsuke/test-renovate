{
  "extends": [
    "config:base",
    "helpers:pinGitHubActionDigests"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
      "fileMatch": ["hello.txt"],
      "matchStrings": [
        "(?<depName>[^\\n]+),(?<currentDigest>[^'\" \\n]+?),(?<currentValue>[^'\" \\n]+?)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
