{
   "extends": [
      "config:recommended",
      "github>suzuki-shunsuke/renovate-config:nolimit#2.2.0"
   ],
   "customManagers": [
      {
         "customType": "regex",
         "datasourceTemplate": "git-refs",
         "depNameTemplate": "suzuki-shunsuke/lintnet",
         "currentValueTemplate": "main",
         "packageNameTemplate": "https://github.com/suzuki-shunsuke/lintnet",
         "fileMatch": [
            "hello\\.txt"
         ],
         "matchStrings": [
            "github-tags>suzuki-shunsuke/lintnet#(?<currentDigest>[^'\" \\n]+)"
         ]
      },
      {
         "customType": "regex",
         // "datasourceTemplate": "git-refs",
         // "currentValueTemplate": "main",
         "datasourceTemplate": "git-tags",
         "fileMatch": [
            "\\.?lintnet\\.ya?ml$"
         ],
         "matchStrings": [
            // - github.com/suzuki-shunsuke/tfcmt/examples/lint/csv.jsonnet@ebd3c7221bf26d51d3cdd0838844dcf14d28ba80 # v4.7.0
            "(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" \\n]+)",
         ]
      }
   ]
}
