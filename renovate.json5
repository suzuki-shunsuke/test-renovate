{
   "extends": [
      "config:recommended",
      "github>suzuki-shunsuke/renovate-config:nolimit#2.2.0"
   ],
   "customManagers": [
      {
         "customType": "regex",
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.?lintnet\\.jsonnet$"
         ],
         "matchStrings": [
            "(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)+)@(?<currentDigest>[0-9a-fA-F]{40}):(?<currentValue>[^'\" @/\\n]+)"
         ]
      },
      {
         "customType": "regex",
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.jsonnet$",
            "\\.libsonnet$"
         ],
         "matchStrings": [
            "^ *local .*= *import '(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)+)@(?<currentDigest>[0-9a-fA-F]{40}):(?<currentValue>[^'\" @/\\n]+)'",
            "^ *local .*= *import \"(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)+)@(?<currentDigest>[0-9a-fA-F]{40}):(?<currentValue>[^'\" @/\\n]+)\""
         ]
      },
      {
         "datasourceTemplate": "github-releases",
         "depNameTemplate": "lintnet/renovate-config",
         "fileMatch": [
            "^renovate\\.json5?$",
            "^\\.github/renovate\\.json5?$",
            "^\\.gitlab/renovate\\.json5?$",
            "^\\.renovaterc\\.json$",
            "^\\.renovaterc$"
         ],
         "matchStrings": [
            "\"github>lintnet/renovate-config#(?<currentValue>[^\" \\n\\(]+)",
            "\"github>lintnet/renovate-config:.*#(?<currentValue>[^\" \\n\\(]+)",
            "\"github>lintnet/renovate-config/.*#(?<currentValue>[^\" \\n\\(]+)"
         ]
      }
   ]
}
