{
  "extends": [
    'github>suzuki-shunsuke/renovate-config:nolimit#2.5.0',
  ],
  "customManagers": [
     {
        "customType": "regex",
        "datasourceTemplate": "github-releases",
        "fileMatch": [
           "^renovate\\.json5?$",
        ],
        "matchStrings": [
           "\"github>(?<depName>(?<packageName>[^/:# \\n]+/[^/:# \\n]+)[^#() \\n]*)#(?<currentValue>[^\" \\n\\(]+)"
        ]
     },
  ],
}
