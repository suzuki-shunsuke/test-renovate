{
   extends: [
      "config:base",
      "github>suzuki-shunsuke/renovate-config:nolimit#2.0.0",
   ],
   "regexManagers": [
      {
         "datasourceTemplate": "crate",
         "fileMatch": [
	    "crate\\.txt",
         ],
	 "depNameTemplate": "skim",
         "matchStrings": [
	    "skim (?<currentValue>[^'\" \\n]+)",
         ],
      },
   ]
}
