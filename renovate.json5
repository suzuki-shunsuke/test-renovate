{
   "extends": [
      "config:recommended",
      "github>suzuki-shunsuke/renovate-config:nolimit#2.2.0"
   ],
   "customManagers": [
      {
         "customType": "regex",
         "datasourceTemplate": "git-refs",
         "depNameTemplate": "suzuki-shunsuke/lintnet",
         "currentValueTemplate": "main",
         "packageNameTemplate": "https://github.com/suzuki-shunsuke/lintnet",
         "fileMatch": [
            "hello\\.txt"
         ],
         "matchStrings": [
            "github-tags>suzuki-shunsuke/lintnet#(?<currentDigest>[^'\" \\n]+)"
         ]
      },
      {
         "customType": "regex",
         "datasourceTemplate": "github-tags",
         "fileMatch": [
            "\\.?lintnet\\.ya?ml$"
         ],
         "matchStrings": [
            // - github.com/suzuki-shunsuke/tfcmt/examples/lint/csv.jsonnet@ebd3c7221bf26d51d3cdd0838844dcf14d28ba80 # v4.7.0
            "(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" @/\\n]+)",
         ]
      },
      {
         "customType": "regex",
         "datasourceTemplate": "github-tags",
         "fileMatch": [
            "\\.jsonnet$",
            "\\.libsonnet$",
         ],
         "matchStrings": [
            // local hello = import 'github.com/suzuki-shunsuke/tfcmt/examples/lint/csv.jsonnet@v4.7.0';
            "^ *local .*= *import '(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" @/\\n]+)'",
            "^ *local .*= *import \"(?<depName>github\\.com/(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" @/\\n]+)\"",
         ]
      },
      // {
      //    "customType": "regex",
      //    "datasourceTemplate": "git-refs",
      //    "currentValueTemplate": "main",
      //    "fileMatch": [
      //       "\\.?lintnet\\.ya?ml$"
      //    ],
      //    "matchStrings": [
      //       // - github.com/suzuki-shunsuke/tfcmt/examples/lint/csv.jsonnet@ebd3c7221bf26d51d3cdd0838844dcf14d28ba80 # v4.7.0
      //       "(?<depName>(?<packageName>github\\.com/[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentDigest>[0-9a-fA-F]{40})",
      //    ]
      // }
   ]
}
